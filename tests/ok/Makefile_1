#!/usr/local/bin/najlo -s
foo = 10

src/najlo.sh: LICENSE
	@echo "HI"

src/najlo_cli.sh:
	@echo "HELLO"
	touch $^

toot:
	@echo "TOOT"

./anvil: toot src/najlo.sh
	@echo -e "\033[1;35m[Makefile]\e[0m    Bootstrapping \"./$anvil\":"


all: src/najlo.sh toot
	@echo "Transmute"
	@echo "Transmute"

%.a: %.o
	@echo "Transmute"
	@echo "Transmute"
	@echo "Transmute"
	@echo "Transmute"

.DEFAULT_GOAL := all

hey = 10
